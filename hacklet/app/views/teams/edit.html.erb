<h1>Editing Team</h1>

<%= render 'form' %>

<%= form_for @invite, :url => invites_path do |f| %>
  <%= f.hidden_field :team_id, :value => @invite.team_id %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.submit 'Send' %>
<% end %>
<%= link_to 'Show', @team %> |
<%= link_to 'Back', teams_path %>
